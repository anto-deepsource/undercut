    "@docusaurus/core": "^2.0.0-alpha.68",
    "@docusaurus/preset-classic": "^2.0.0-alpha.68",
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "browserslist": {
