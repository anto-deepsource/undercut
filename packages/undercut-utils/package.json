{
  "name": "@undercut/utils",
  "version": "0.6.1",
  "description": "Utilities for type checking, arrays, comparison, common functions, iterables, objects, promises, randomization, etc.",
  "author": "Anton Alexandrenok",
  "license": "MIT",
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./array": "./src/array.ts",
    "./assert": "./src/assert.ts",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": ". scripts/build.sh",
    "build:babel": "babel src --extensions '.js','.ts' --root-mode upward --ignore \"src/**/*.test.ts\",\"src/**/*.test.*.ts\"",
    "build:types": "tsc --emitDeclarationOnly",
    "clear": "rm -rf build dist",
    "dist": ". scripts/dist.sh",
    "lint": "eslint --format='unix' '**/*.@(cjs|mjs|js|ts)'",
    "test": "jest",
    "ts": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.16",
    "@jest/globals": "^26.6.2",
    "@undercut/config": "^1.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-add-import-extension": "^1.4.3",
    "core-js": "^3.8.1",
    "eslint": "^7.15.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "promise-mock": "^2.1.0",
    "typescript": "^4.1.5"
  },
  "homepage": "https://undercut.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/the-spyke/undercut",
    "directory": "packages/undercut-utils"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "array",
    "es6",
    "esm",
    "esnext",
    "generator",
    "iterable",
    "iterator",
    "itertools",
    "lodash",
    "observer",
    "stdlib",
    "underscore",
    "utils"
  ]
}
