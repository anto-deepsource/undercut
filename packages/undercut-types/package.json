{
  "name": "@undercut/types",
  "version": "0.6.1",
  "description": "TypeScript types used in the Undercut",
  "author": "Anton Alexandrenok",
  "license": "MIT",
  "type": "module",
  "main": "./src/index.ts",
  "sideEffects": false,
  "scripts": {
    "build": ". scripts/build.sh",
    "build:babel": "babel src --root-mode upward --ignore \"src/**/*.test.js\",\"src/**/*.test.*.js\"",
    "build:types": "tsc --emitDeclarationOnly",
    "clear": "rm -rf build dist",
    "dist": ". scripts/dist.sh",
    "lint": "eslint --format='unix' '**/*.@(cjs|mjs|js|ts)'",
    "test": "tsc --noEmit",
    "ts": "tsc --noEmit"
  },
  "devDependencies": {
    "@undercut/config": "^1.0.0",
    "eslint": "^7.15.0",
    "typescript": "^4.1.5"
  },
  "homepage": "https://undercut.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/the-spyke/undercut",
    "directory": "packages/undercut-types"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src/",
    "LICENSE",
    "README.md"
  ]
}
