{
  "name": "@undercut/cli",
  "version": "0.6.1",
  "description": "A CLI for lazy data processing in a shell using operations from Undercut and any valid JavaScript expression.",
  "author": "Anton Alexandrenok",
  "license": "MIT",
  "type": "module",
  "main": "./src/index.js",
  "bin": {
    "undercut": "./src/cli.js"
  },
  "engines": {
    "node": ">=12.17"
  },
  "scripts": {
    "build": ". scripts/build.sh",
    "clear": "rm -rf build dist",
    "dist": ". scripts/dist.sh",
    "lint": "eslint --format='unix' '**/*.?(c|m)js'",
    "prepack": "test -d lib",
    "test": "jest"
  },
  "dependencies": {
    "@undercut/pull": "^0.6.1",
    "@undercut/push": "^0.6.1",
    "@undercut/utils": "^0.6.1",
    "core-js": "^3.8.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@jest/globals": "^26.6.2",
    "@undercut/config": "^1.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.15.0",
    "jest": "^26.6.3",
    "semver": "^7.3.4"
  },
  "homepage": "https://undercut.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/the-spyke/undercut",
    "directory": "packages/undercut-cli"
  },
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "files": [
    "lib/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "cli",
    "console",
    "filter",
    "lodash",
    "map",
    "pipe",
    "reduce",
    "shell",
    "stdin",
    "stdlib",
    "stdout",
    "termial",
    "underscore"
  ]
}
