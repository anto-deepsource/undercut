{
  "name": "@undercut/cli",
  "version": "0.5.0",
  "description": "Process data with the Undercut and JavaScript in a shell",
  "author": "Anton Alexandrenok",
  "license": "MIT",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "undercut": "src/cli.js"
  },
  "engines": {
    "node": ">=10.13"
  },
  "scripts": {
    "build": ". scripts/build.sh",
    "clear": "rm -rf build dist",
    "dist": ". scripts/dist.sh",
    "lint": "eslint --format='unix' '**/*.?(c|m)js'",
    "prepack": "test -d lib",
    "test": "jest"
  },
  "dependencies": {
    "@undercut/node-10": "^0.5.0",
    "core-js": "^3.6.5",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@jest/globals": "^26.0.1",
    "@undercut/config": "^0.5.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.1.0",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "semver": "^7.3.2"
  },
  "homepage": "https://undercut.js.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/the-spyke/undercut",
    "directory": "packages/undercut-cli"
  },
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "files": [
    "lib/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "cli",
    "console",
    "data",
    "filter",
    "map",
    "pipe",
    "reduce",
    "shell",
    "stdin",
    "stdout",
    "termial",
    "undercut"
  ]
}
