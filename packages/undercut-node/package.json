{
  "name": "@undercut/node",
  "version": "0.6.1",
  "description": "Lazy data processing, pipelines, and language utilities built around native JS features and protocols.",
  "author": "Anton Alexandrenok",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./node": "./src/index.js",
    "./pull": "./exports/pull.js",
    "./push": "./exports/push.js",
    "./utils": "./exports/utils.js",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=12.17"
  },
  "scripts": {
    "build": ". scripts/build.sh",
    "build:babel": "babel src --source-maps --ignore \"src/**/*.test.js\",\"src/**/*.test.*.js\"",
    "clear": "rm -rf build dist",
    "dist": ". scripts/dist.sh",
    "lint": "eslint --format='unix' '**/*.?(c|m)js'",
    "prepack": "test ! -d scripts",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@jest/globals": "^26.6.2",
    "@undercut/config": "^1.0.0",
    "@undercut/pull": "^0.6.1",
    "@undercut/push": "^0.6.1",
    "@undercut/utils": "^0.6.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "core-js": "^3.8.1",
    "eslint": "^7.15.0",
    "jest": "^26.6.3",
    "semver": "^7.3.4"
  },
  "homepage": "https://undercut.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/the-spyke/undercut",
    "directory": "packages/undercut-node"
  },
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "files": [
    "*.js",
    "*.js.map",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "array",
    "collection",
    "data structure",
    "filter",
    "generator",
    "iterable",
    "iterator",
    "itertools",
    "lazy",
    "linq",
    "lodash",
    "map",
    "node",
    "observer",
    "pipeline",
    "reduce",
    "stdlib",
    "underscore"
  ]
}
